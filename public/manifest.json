{
  "name": "COD API Read",
  "description": "Read COD API",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Open menu"
  },
  "host_permissions": ["https://my.callofduty.com/"],
  "permissions": ["activeTab", "scripting", "cookies"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
